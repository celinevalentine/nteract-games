{
    "$schema": "http://json-schema.org/draft-07/schema",
    "$id": "http://example.com/example.json",
    "type": "object",
    "title": "The root schema",
    "description": "The root schema comprises the entire JSON document.",
    "default": {},
    "examples": [
        {
           
                "hotspot_name": "grid11",
                "x": 38,
                "y": 600,
                "width": 291,
                "height": 170
            
        }
    ],
    
    "properties": {
        "hotspot": {
            "$id": "#/properties/hotspot",
