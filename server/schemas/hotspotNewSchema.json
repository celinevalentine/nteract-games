                        600
                    ]
                },
                "width": {
                    "$id": "#/properties/hotspot/properties/width",
                    "type": "integer",
                    "title": "The width schema",
                    "description": "An explanation about the purpose of this instance.",
                    "default": 0,
                    "examples": [
                        291
                    ]
                },
                "height": {
                    "$id": "#/properties/hotspot/properties/height",
                    "type": "integer",
