{
    "$schema": "http://json-schema.org/draft-07/schema",
    "$id": "http://example.com/example.json",
    "type": "object",
    "title": "The root schema",
    "description": "The root schema comprises the entire JSON document.",
    "default": {},
    "examples": [
        {
            "hotspot_name": "grid11",
            "x": 13,
            "y": 38,
            "width": 291,
            "height": 170
        }
    ],
    "required": [
        "hotspot_name",
        "x",
        "y",
        "width",
        "height"
    ],
    "properties": {
        "hotspot_name": {
            "$id": "#/properties/hotspot_name",
            "type": "string",
            "title": "The hotspot_name schema",
            "description": "An explanation about the purpose of this instance.",
            "default": "",
            "examples": [
                "grid11"
            ]
        },
        "x": {
            "$id": "#/properties/x",
            "type": "integer",
            "title": "The x schema",
            "description": "An explanation about the purpose of this instance.",
            "default": 0,
            "examples": [
                13
            ]
        },
        "y": {
            "$id": "#/properties/y",
            "type": "integer",
            "title": "The y schema",
            "description": "An explanation about the purpose of this instance.",
            "default": 0,
            "examples": [
                38
            ]
        },
        "width": {
            "$id": "#/properties/width",
            "type": "integer",
            "title": "The width schema",
            "description": "An explanation about the purpose of this instance.",
            "default": 0,
            "examples": [
                291
            ]
        },
        "height": {
            "$id": "#/properties/height",
            "type": "integer",
            "title": "The height schema",
            "description": "An explanation about the purpose of this instance.",
            "default": 0,
            "examples": [
                170
            ]
        }
    },
    "additionalProperties": true
}